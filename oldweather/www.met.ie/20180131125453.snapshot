function SwapMapType(MapType)
{
	//store the value now in the hidden box
	document.MetData.wcurrtype.value = MapType;
	//Get the correct date from the hidden box
	var ThisDay = document.MetData.wcurrdate.value;

	document.metmap.src=imgpath+'nat0' + ThisDay +'_'+ GetTypeToName(MapType) +'.png?1254'
}

// function to switch the map to another day
function SwapMapDay(ThisDay)
{
	//store the value now in the hidden box
	document.MetData.wcurrdate.value = ThisDay;
	//Get the correct date from the hidden box
	var MapType = document.MetData.wcurrtype.value * 1;

	document.metmap.src=imgpath+'nat0' + ThisDay +'_'+ GetTypeToName(MapType) +'.png?1254'
}


// function to convert the type (1,2,3) to its name value (weather,wind,temp)
function GetTypeToName(MapType)
	{
	switch (MapType)
		{
		  case 1:
			return "weather" ;
			break;
		  case 2:
			return "wind" ;
			break;
		  case 3:
			return "temp" ;
			break;
		}
